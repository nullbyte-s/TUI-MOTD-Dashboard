#
# Run-time configuration file for dialog
#
# Automatically generated by "dialog --create-rc <file>"
#
#
# Types of values:
#
# Number     -  <number>
# String     -  "string"
# Boolean    -  <ON|OFF>
# Attribute  -  (foreground,background,highlight?)

# Set aspect-ration.
aspect = 0

# Set separator (for multiple widgets output).
separate_widget = ""

# Set tab-length (for textbox tab-conversion).
tab_len = 0

# Make tab-traversal for checklist, etc., include the list.
visit_items = OFF

# Shadow dialog boxes? This also turns on color.
use_shadow = OFF

# Turn color support ON or OFF
use_colors = ON

# Screen color
screen_color = (BLUE,BLACK,ON)

# Shadow color
shadow_color = (BLACK,BLACK,OFF)

# Dialog color
dialog_color = (BLUE,BLACK,ON)

# Dialog box title color
title_color = (RED,BLACK,ON)

# Dialog box border color
border_color = (RED,BLACK,OFF)
border2_color = border_color

# Active button color
button_active_color = (CYAN,BLUE,ON)

# Inactive button color
button_inactive_color = (RED,BLACK,ON)

# Active button key color
button_key_active_color = (BLACK,MAGENTA,ON)

# Inactive button key color
button_key_inactive_color = (RED,BLACK,ON)

# Active button label color
button_label_active_color = button_active_color

# Inactive button label color
button_label_inactive_color = button_inactive_color

# Input box color
inputbox_color = (RED,BLACK,ON)

# Input box border color
inputbox_border_color = (RED,BLACK,ON)

# Search box color
searchbox_color = (RED,BLACK,ON)

# Search box title color
searchbox_title_color = title_color

# Search box border color
searchbox_border_color = border_color

# File position indicator color
position_indicator_color = title_color

# Menu box color
menubox_color = (BLACK,BLACK,OFF)

# Menu box border color
menubox_border_color = (MAGENTA,BLACK,ON)

# Item color
item_color = (YELLOW,BLACK,ON)

# Selected item color
item_selected_color = (CYAN,BLUE,ON)

# Tag color
tag_color = title_color

# Selected tag color
tag_selected_color = button_label_active_color

# Tag key color
tag_key_color = button_key_inactive_color

# Selected tag key color
tag_key_selected_color = (RED,BLUE,ON)

# Check box color
check_color = menubox_border_color

# Selected check box color
check_selected_color = button_active_color

# Up arrow color
uarrow_color = (GREEN,BLACK,OFF)

# Down arrow color
darrow_color = uarrow_color

# Item help-text color
itemhelp_color = (MAGENTA,BLACK,OFF)

# Active form text color
form_active_text_color = button_active_color

# Form text color
form_text_color = (MAGENTA,CYAN,ON)

# Readonly form item color
form_item_readonly_color = (CYAN,MAGENTA,ON)

# Dialog box gauge color
gauge_color = title_color

# Dialog box border2 color
border2_color = (RED,BLACK,OFF)

# Input box border2 color
inputbox_border2_color = menubox_border_color

# Search box border2 color
searchbox_border2_color = menubox_border_color

# Menu box border2 color
menubox_border2_color = menubox_border_color
